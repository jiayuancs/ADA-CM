                "--file1", "hicodet_pkl_files/union_embeddings_cachemodel_crop_padding_zeros_vitb16.p",
                "--clip_dir_vit", "checkpoints/pretrained_clip/ViT-B-16.pt"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES":"3"
            }
        }
    ]
}

